#include <stdio.h>
int main() {
    int m[10][10]; 
    int i, j, x;
    int value = 1;
    for (i = 0; i < 10; i++) {
        for (j = 0; j < 10; j++) {
            m[i][j] = value++;
        }  }
    for (i = 0; i < 10; i++) {
        for (j = 0; j < 10; j++) {
            if (i == j) { // إذا كان الموقع على القطر الرئيسي
                m[i][j] = -20;
            } else if (i > j) { // إذا كان العنصر تحت القطر الرئيسي
                x = m[i][j];         // حفظ القيمة مؤقتًا
                m[i][j] = m[j][i];   // تبديل القيمة مع العنصر المقابل فوق القطر
                m[j][i] = x;         // إعادة القيمة المحفوظة
            }  }  }

    printf(" Le Matrice :\n");
    for (i = 0; i < 10; i++) {
        for (j = 0; j < 10; j++) {
            printf("%d\t", m[i][j]);
        }
        printf("\n");
    }
    return 0;
}
