#include <stdio.h>
int main() {
    int m[4][4]; 
    int i, j, x;
    int value = 1;
    for (i = 0; i < 4; i++) {
        for (j = 0; j < 4; j++) {
            m[i][j] = value++;
        }  }
    for (i = 0; i < 4; i++) {
        for (j = 0; j < 4; j++) {
             if (i >= j) { 
                x = m[i][j];        
                m[i][j] = m[j][i];  
                m[j][i] = x;     
            }
         } 
      }
    printf(" Le Matrice :\n");
    for (i = 0; i < 4; i++) {
        for (j = 0; j < 4; j++) {
            printf("%d\t", m[i][j]);
        }
        printf("\n");
    }
    return 0;
}
